{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "object-curly-spacing": 0,
    "import/no-dynamic-require": 0,
    "function-paren-newline": 0,
    "comma-dangle": 0,
    "arrow-parens": [
      "error",
      "always"
    ]
  },
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "node": true
  }
}
